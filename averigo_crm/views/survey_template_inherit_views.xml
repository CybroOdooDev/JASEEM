<?xml version="1.0" encoding="utf-8" ?>
<odoo>


    <template id="question_container_site_survey"
              inherit_id="survey.question_container">
        <xpath expr="//div[@t-att-id='question.id']"
               position="inside">
            <t t-if="question.question_type == 'images'">
                <t t-call="averigo_crm.images_print"/>
            </t>

        </xpath>
    </template>

    <template id="images_print" name="Images Print">
        <div>

            <input type="file" accept="image/*" id="imageUpload"
                   name="imageUpload"/>

        </div>


    </template>


</odoo>